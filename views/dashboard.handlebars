<div id="Blog Posts">
    <h2>Your Dashboard</h2>
    <ul>
    {{#each post in posts}}
    <div class="postTitle">
        <h3>${post.title}</h3>
    </div>
    <div class="postDate">
        <h3>${post.date}</h3>
    </div>
    {{//each}}
    </ul>
    <button type="button" value="newPost">+ New Post</button>
</div>
